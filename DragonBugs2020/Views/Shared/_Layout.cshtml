@{
    var message = (string)TempData["DemoLockout"];
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>
        Whack-A-Bug
    </title>
    <link rel="stylesheet" href="~/css/bootstrap.min.css" />
    <link href="~/css/icons.min.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="~/css/site.css" />


    <link href="~/css/app.min.css" id="app-stylesheet" rel="stylesheet" type="text/css" />
    <link href="~/css/app-dark.css" rel="stylesheet" />
    <link href="~/css/icons.css" rel="stylesheet" />
    <link href="~/css/bootstrap-dark.css" rel="stylesheet" />


    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.22/css/jquery.dataTables.css">
    <link href="~/css/responsive.bootstrap4.css" rel="stylesheet" />
    <link href="~/css/bootstrap.min.css" id="bootstrap-stylesheet" rel="stylesheet" type="text/css" />

    <link href="~/css/Custom.css" rel="stylesheet" />

    <link src="~/favicon.ico" />


</head>
<body>


    <header>
        <nav class="my-nav navbar navbar-expand-sm navbar-toggleable-sm border-bottom box-shadow mb-3">
            <div class="container my-nav">
                <img src="~/img/nav-title-only.png" />
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <partial name="_LoginPartial" />
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link my-nav" asp-area="" asp-controller="Home" asp-action="Dashboard">Dashboard</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link my-nav" asp-area="" asp-controller="UserRoles" asp-action="ManageUserRoles">The Team</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link my-nav" asp-area="" asp-controller="Projects" asp-action="Index">Projects</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link my-nav" asp-area="" asp-controller="Projects" asp-action="MyProjects">My Projects</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link my-nav" asp-area="" asp-controller="Tickets" asp-action="Index">Tickets</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link my-nav" asp-area="" asp-controller="Tickets" asp-action="MyTickets">MyTickets</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>



    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>


    @*<footer>
            <section>
                2016 - 2020 &copy; Adminto theme by <a href="">Coderthemes</a><br />
                &copy; 2020 - DragonBugs2020 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                <br /><br /><br /><br />
            </section>
        </footer>*@







    <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <script src="~/js/vendor.min.js"></script>
    <script src="~/js/jquery.knob.min.js"></script>
    <script src="~/js/morris.min.js"></script>
    <script src="~/js/raphael.min.js"></script>
    <script src="~/js/dashboard.init.js"></script>
    <script src="~/js/datatables.init.js"></script>
    <script src="~/js/app.min.js"></script>
    
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.22/js/jquery.dataTables.js"></script>


    <script>
        $(document).ready(function () {
            $('#responsive-datatable').DataTable();
        });
    </script>
    @if (!string.IsNullOrWhiteSpace(message))
    {
        <script>
            function alertFn() {
                alert("@TempData["DemoLockout"]");
            }
        </script>
    }
    @RenderSection("Scripts", required: false)
</body>
</html>
